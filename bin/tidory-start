#!/usr/bin/env node
/* eslint-disable no-console */

const shelljs = require('shelljs')
const path = require('path');

const _webpackDevConfig = path.join(__dirname, '../dev/webpack.dev.conf');
const _webpack = path.join(__dirname, '../node_modules/webpack-dev-server/bin/webpack-dev-server.js');

shelljs.exec(`node ${_webpack} --config ${_webpackDevConfig}`);